{
  "name": "dataviz",
  "version": "1.0.0",
  "description": "Data visualization tool for E02.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "scss": "node-sass --watch assets/scss -o assets/css",
    "package-mac": "electron-packager . Nautilus --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Nautilus --overwrite --asar --platform=win32 --arch=x64 --icon=assets/icons/win/Icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nautilus\"",
    "package-linux": "electron-packager . Nautilus --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Atharva Pusalkar",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "chartist-plugin-annotation": "0.0.7",
    "chartist-plugin-tooltip": "0.0.11",
    "custom-electron-titlebar": "^3.2.2-hotfix62",
    "darkmode-js": "^1.5.5",
    "jquery": "^3.5.0",
    "node-sass": "^4.14.0",
    "popper.js": "^2.0.0-next.2",
    "serialport": "^8.0.7"
  },
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1",
    "electron-package": "^0.1.0",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1"
  }
}

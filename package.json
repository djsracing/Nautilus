{
  "name": "Nautilus",
  "version": "1.0.0",
  "description": "Data visualization tool for E02.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron --enable-accelerated-mjpeg-decode --enable-accelerated-video --ignore-gpu-blacklist --enable-native-gpu-memory-buffers --enable-gpu-rasterization .",
    "scss": "node-sass --watch assets/scss -o assets/css",
    "package-mac": "electron-packager . Nautilus --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Nautilus --overwrite --platform=win32 --arch=x64 --icon=assets/icons/win/Icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Nautilus\"",
    "package-linux": "electron-packager . Nautilus --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Atharva Pusalkar",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "electron-installer-windows": "^3.0.0",
    "jquery": "^3.5.0",
    "node-sass": "^4.14.1",
    "popper.js": "^2.0.0-next.2",
    "select2": "^4.0.13",
    "serialport": "^8.0.7",
    "sweetalert2": "^9.10.12",
    "yargs-parser": "^18.1.3"
  },
  "devDependencies": {
    "electron": "^8.5.2",
    "electron-builder": "^22.5.1",
    "electron-packager": "^14.2.1",
    "electron-rebuild": "^1.10.1",
    "electron-updater": "^4.3.1",
    "socket.io-client": "^2.3.0"
  }
}
